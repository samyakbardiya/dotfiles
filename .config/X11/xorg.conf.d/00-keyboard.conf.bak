Section "InputClass"
	Identifier "system-keyboard"
	MatchIsKeyboard "on"
	Option "XkbModel" "pc104"
	Option "XkbLayout" "us,in"
	Option "XkbVariant" "dvp,eng"
	Option "XkbOptions" "grp:alt_space_toggle, grp_led:caps, grp_led:scroll, caps:escape, compose:sclk, shift:both_shiftlock keypad:pointerkeys, rupeesign:i1, altwin:swap_lalt_lwin, keypad:pointerkeys, terminate:ctrl_alt_bksp"
EndSection
