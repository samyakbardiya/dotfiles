{{ if eq .chezmoi.os "darwin" -}}
#!/usr/bin/env bash

brew bundle --file=/dev/stdin <<EOF
{{ range .homebrew.taps -}}
tap {{ . | quote }}
{{ end -}}
{{ range .homebrew.brews -}}
brew {{ . | quote }}
{{ end -}}
{{ range .homebrew.casks -}}
cask {{ . | quote }}
{{ end -}}
EOF

# vim:ft=sh
{{- end }}
