[
  {
    "context": "vim_mode == visual",
    "bindings": {
      "shift-s": [
        "vim::PushOperator",
        {
          "AddSurrounds": {}
        }
      ]
    }
  },
  {
    "context": "Editor && VimControl && !VimWaiting && !menu",
    "bindings": {
      "K": "editor::Hover",
      "g r": "editor::GoToImplementation",
      "shift-l": "pane::ActivateNextItem",
      "shift-h": "pane::ActivatePrevItem"
    }
  }
]
